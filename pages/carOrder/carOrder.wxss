/* pages/carOrder/carOrder.wxss */
.title-box {
  margin: 10rpx 10rpx 0 10rpx;
  font-size: 36rpx;
}

.display-box {
  height: 100%;
  margin: 20rpx 10rpx 10rpx 10rpx;
  flex: 1;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 600rpx;
  border-radius: 6px;
}

.T1pool{
  margin-right: 10rpx;
}

.T2pool{
  margin-left: 10rpx;
}

.T1pool,.T2pool {
  width: 50%;
  height: 98.8%;
  background-color: rgba(255, 255, 255, 1);
  padding: 4px;
  border-radius: 6px;
  font-size: 34rpx;
}

.order-item{
  display: inline-block;
  width: 30%;
  height: 30px;
  line-height: 30px;
  text-align: center;
}

.carNo-item{
  display: inline-block;
  width: 60%;
  height: 30px;
  line-height: 30px;
  font-size: 32rpx;
}